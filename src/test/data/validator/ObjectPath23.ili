INTERLIS 2.3;

MODEL ObjectPath23
  AT "mailto:ce@eisenhutinformatik.ch" VERSION "2016-11-16" =
	
	TOPIC Topic=

		DOMAIN
			Coord = COORD 1..10,1..10;

		CLASS ClassC1 =
			attrC1 : TEXT*10;
		END ClassC1;
		
		STRUCTURE SubSubStruct =
			attrS8 : TEXT*10;
			attrS9 : TEXT*10;
		END SubSubStruct;
		STRUCTURE StructS =
			attrS1 : TEXT*10;
			attrS2 : TEXT*10;
			attrS3 : SubSubStruct;
		END StructS;
		STRUCTURE StructD1 =
			attrD1 : TEXT*10;
			attrD2 : REFERENCE TO ClassC1;
			attrD3 : StructS;
			attrD4 : StructS;
			attrD5 : Coord;
			attrD6 : Coord;
		END StructD1;

		CLASS ClassB =
		   attrB1 : TEXT*10;
		   attrB2 : StructD1;
		   attrB3 : LIST {0..*} OF StructD1;
		   MANDATORY CONSTRAINT attrB2->attrD1 == "REF";
		   MANDATORY CONSTRAINT attrB3[FIRST]->attrD1 == "REF";
		END ClassB;

		CLASS ClassE =
		   attrE1 : TEXT*10;
		   attrE2 : StructD1;
		   MANDATORY CONSTRAINT attrE2->attrD3 == attrE2->attrD4;
		END ClassE;

		CLASS ClassF =
		   attrF1 : TEXT*10;
		   attrF2 : StructD1;
		   MANDATORY CONSTRAINT attrF2->attrD5 == attrF2->attrD6;
		END ClassF;
		
		CLASS ClassG =
		   attrB1 : TEXT*10;
		   attrB2 : StructD1;
		   MANDATORY CONSTRAINT attrB2->attrD2->attrC1 == "REF";
		END ClassG;

		CLASS ClassH =
			attrH1 : TEXT*10;
		END ClassH;

		ASSOCIATION one2many =
			roleH_1 -- {0..1} ClassH;
			roleC_1 -- {0..*} ClassC1;
		END one2many;
		CONSTRAINTS OF ClassC1 =
			MANDATORY CONSTRAINT roleH_1->attrH1 == "H1";
		END;

		ASSOCIATION many2many =
			roleH_2 -- {0..*} ClassH;
			roleC_2 -- {0..*} ClassC1;
			MANDATORY CONSTRAINT roleH_2->attrH1 == "H1";
		END many2many;
		
		CLASS ClassS =
		  s1 : TEXT*10;
		END ClassS;
		
		CLASS ClassT =
		  t1 : TEXT*10; 
		END ClassT;

		ASSOCIATION st1 =
		  role_s1 -- {0..*} ClassS;
		  role_t1 -- {0..*} ClassT;
		  st_1 : TEXT*10;
		END st1;

		!!CONSTRAINTS OF ClassS =
		!!	MANDATORY CONSTRAINT \role_t1->st_1 == "value_st1"; 
		!!END;

		!! CONSTRAINTS OF ClassS =
		!!	MANDATORY CONSTRAINT role_t1->t1 == "value_t1";
		!! END;

		CLASS ClassU =
		  u1 : TEXT*10; 
		END ClassU;

		ASSOCIATION tu =
			role_s3 -- {0..*} ClassS;
			role_u3 -- {0..1} ClassU;
		END tu;

		CONSTRAINTS OF ClassU =
			MANDATORY CONSTRAINT INTERLIS.objectCount(role_s3->role_t1)==4;
		END;
		
		CLASS ClassV =
			v1 : TEXT*10;
		END ClassV;
		CLASS ClassW =
		END ClassW;
		ASSOCIATION vw =
			role_v -- {0..1} ClassV;
			role_w -- {0..2} ClassW;
			MANDATORY CONSTRAINT DEFINED(role_v->v1);
		END vw;

	END Topic;

	TOPIC ExpressionSingleValue = 

		CLASS ClassS =
		  s1 : TEXT*10;
		END ClassS;
		
		CLASS ClassT =
		  t1 : TEXT*10; 
		END ClassT;

		CLASS ClassU =
		  u1 : TEXT*10; 
		END ClassU;

		ASSOCIATION st1 =
		  role_s1 -- {0..*} ClassS;
		  role_t1 -- {0..*} ClassT;
		  st_1 : TEXT*10;
		END st1;

		ASSOCIATION tu =
			role_s3 -- {0..*} ClassS;
			role_u3 -- {0..1} ClassU;
		END tu;

		CONSTRAINTS OF ClassS =
			MANDATORY CONSTRAINT role_t1->t1 == "value_t1";
		END;

	END ExpressionSingleValue;

	TOPIC BackwardDirection =

		CLASS ClassS =
		  s1 : TEXT*10;
		END ClassS;
		
		CLASS ClassT =
		  t1 : TEXT*10; 
		END ClassT;

		ASSOCIATION st1 =
		  role_s1 -- {0..*} ClassS;
		  role_t1 -- {0..*} ClassT;
		  st_1 : TEXT*10;
		END st1;

		ASSOCIATION st2 =
			role_s2 -- {0..1} ClassS;
			role_t2 -- {0..*} ClassT;
		END st2;

		CONSTRAINTS OF ClassS =
			MANDATORY CONSTRAINT \role_t1->st_1 == "value_st1"; 
		END;

		CONSTRAINTS OF ClassS =
			MANDATORY CONSTRAINT role_t2->t1 == "value_t1";
		END;
		
		CLASS ClassV =
		END ClassV;
		CLASS ClassW =
			w1 : TEXT*10;
		END ClassW;
		ASSOCIATION vw =
			role_v -- {0..1} ClassV;
			role_w -- {0..2} ClassW;
			MANDATORY CONSTRAINT DEFINED(role_w->w1);
		END vw;
		
	END BackwardDirection;	

	TOPIC ForwardDirectionWithMoreThanOnelinkObj =

		CLASS ClassX =
		  x1 : TEXT*10; 
		END ClassX;		

		CLASS ClassY =
		  y1 : TEXT*10;
		END ClassY;

		CLASS ClassY1 EXTENDS ClassY =
		  y1_1 : TEXT*10;
		END ClassY1;
		
		CLASS ClassZ =
		  z1 : TEXT*10; 
		END ClassZ;

		ASSOCIATION x2y =
			role_x -- {0..*} ClassX;
			role_y -- {0..*} ClassY;
		END x2y;

		ASSOCIATION y2z =
			role_y -- {0..*} ClassY;
			role_z -- {0..*} ClassZ;
		END y2z;

		CONSTRAINTS OF ClassX =
			MANDATORY CONSTRAINT INTERLIS.objectCount(role_y->role_z)==4;
		END;

	END ForwardDirectionWithMoreThanOnelinkObj;

	TOPIC RekursivelyObject = 

		CLASS ClassA =
		  isLeaf : MANDATORY BOOLEAN;
		  a1 : TEXT*10; 
		END ClassA;

		ASSOCIATION a2a =
			role_parent -- {0..*} ClassA;
			role_child -- {0..*} ClassA;
		END a2a;

		CONSTRAINTS OF ClassA =
			MANDATORY CONSTRAINT isLeaf OR INTERLIS.objectCount(role_child) == 2;
		END;
		
	END RekursivelyObject;

END ObjectPath23.